<AnchorPane maxWidth="680.0" stylesheets="@../Styles/styles.css" prefHeight="390.0" prefWidth="505.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainMenuController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="540.0" layoutY="-4.0" opacity="0.88" preserveRatio="true">
         <image>
            <Image url="@../Images/menu-backgrounds/mainMenu.jpg" />
         </image>
      </ImageView>
      <Button layoutX="369.0" layoutY="97.0" mnemonicParsing="false" onMouseClicked="#startGame" prefHeight="26.0" prefWidth="120.0" text="Start Game!">
